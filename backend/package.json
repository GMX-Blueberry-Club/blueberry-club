{
    "name": "backend",
    "version": "0.2.0",
    "license": "MIT",
    "main": ".dist/cjs/server.js",
    "private": true,
    "files": [
        ".dist"
    ],
    "types": ".dist/types/index.d.ts",
    "scripts": {
        "start": "node .dist/cjs/server",
        "dev": "tsnd -r dotenv/config --respawn ./src/server.ts dotenv_config_path=../.env",
        "clean": "yarn g:tsc:clean",
        "build:project": "yarn g:tsc:project",
        "build:cjs": "yarn g:tsc:cjs",
        "build": "yarn build:cjs",
        "bundle": "yarn bundle --no-compress --output-directory ../../www"
    },
    "dependencies": {
        "@aelea/core": "1.3.0",
        "@ethersproject/abi": "5.6.4",
        "@ethersproject/abstract-signer": "5.6.2",
        "@ethersproject/bignumber": "^5.6.2",
        "@ethersproject/bytes": "5.6.1",
        "@ethersproject/contracts": "^5.6.2",
        "@ethersproject/providers": "5.6.8",
        "@gambitdao/gmx-middleware": "workspace:*",
        "@most/core": "1.6.1",
        "@most/disposable": "1.3.0",
        "@most/prelude": "1.8.0",
        "@most/scheduler": "1.3.0",
        "@most/types": "1.1.0",
        "@types/compression": "^1.7.2",
        "@types/isomorphic-fetch": "^0.0.36",
        "@urql/core": "^2.5.0",
        "compression": "^1.7.4",
        "concurrently": "^7.2.2",
        "cors": "^2.8.5",
        "ethers": "^5.6.9",
        "express": "^4.18.1",
        "express-promise-router": "^4.1.1",
        "express-session": "^1.17.3",
        "graphql": "^16.5.0",
        "isomorphic-fetch": "^3.0.0",
        "ws": "^8.8.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/express-serve-static-core": "^4.17.29",
        "@types/express-session": "^1.17.4",
        "@types/node": "18.0.0",
        "@types/ws": "^8.5.3",
        "dotenv": "^16.0.1",
        "ts-node": "^10.8.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.7.4"
    },
    "mikro-orm": {
        "useTsNode": true,
        "configPaths": [
            "./app/mikro-orm.config.ts",
            "./dist/mikro-orm.config.js"
        ]
    },
    "engines": {
        "node": "16.4.x"
    }
}
